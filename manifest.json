{
  "name": "Geforce NOW Russia Chrome Extension",
  "description": "Geforce NOW Russia Compatibility Steam Games",
  "version": "1.0",
  "author": "Ivan Sidorov",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["assets/img/btn.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://store.steampowered.com/app/*"],
      "css": ["style.css"],
      "js": ["app.js"]
    }
  ],
  "icons": {
    "16": "/assets/img/icons/icon-16.png",
    "32": "/assets/img/icons/icon-32.png",
    "48": "/assets/img/icons/icon-48.png",
    "72": "/assets/img/icons/icon-72.png",
    "128": "/assets/img/icons/icon-128.png"
  }
}
